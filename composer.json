{
  "name": "bizley/quill",
  "description": "Quill editor implementation for Yii 2.",
  "type": "yii2-extension",
  "keywords": ["yii2","quill","wysiwyg","widget","editor"],
  "license": "Apache-2.0",
  "authors": [
    {
      "name": "Pawel Bizley Brzozowski",
      "email": "pawel@positive.codes"
    }
  ],
  "support": {
    "source": "https://github.com/bizley/yii2-quill"
  },
  "require": {
    "php": "^7.0",
    "yiisoft/yii2": "^2.0.0",
    "npm-asset/quill": "^1.3.7"
  },
  "require-dev": {
    "roave/security-advisories": "dev-master",
    "npm-asset/highlight.js": "^9.18.1",
    "npm-asset/katex": "^0.11.1"
  },
  "suggest": {
    "npm-asset/highlight.js": "To use syntax highlighting",
    "npm-asset/katex": "To use formula module"
  },
  "autoload": {
    "psr-4": {
      "bizley\\quill\\": "src/"
    }
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    }
  ]
}
